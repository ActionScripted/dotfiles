	"workbench.editorAssociations": {
		"*.ipynb": "jupyter.notebook.ipynb"
	},
	"[python]": {
		"editor.defaultFormatter": "ms-python.python"
	},
	"editor.wordWrap": "off",
	"terminal.external.osxExec": "iTerm.app",
	"terminal.integrated.confirmOnExit": "hasChildProcesses",
	"workbench.colorCustomizations": {
		"terminal.ansiBrightBlack": "#002b36",
		"terminal.ansiBrightBlue": "#839496",
		"terminal.ansiBrightCyan": "#93a1a1",
		"terminal.ansiBrightGreen": "#586e75",
		"terminal.ansiBrightMagenta": "#6c71c4",
		"terminal.ansiBrightRed": "#cb4b16",
		"terminal.ansiBrightWhite": "#fdf6e3",
		"terminal.ansiBrightYellow": "#657b83",
		"terminal.ansiWhite": "#eee8d5",
		"terminal.background": "#002b36",
		"terminal.foreground": "#839496",
		"terminal.selectionBackground": "#073642"
	},
	"vim.enableNeovim": true,
	"vim.neovimUseConfigFile": true,
	"zeplin.preferredApplicationType": "Web",
	"vsnotes.defaultNotePath": "~/Notes",
	"vsnotes.tokens": [
		{
			"type": "datetime",
			"token": "{dt}",
			"format": "YYYY-MM-DD",
			"description": "Insert formatted datetime."
		},
		{
			"type": "title",
			"token": "{title}",
			"description": "Insert note title from input box.",
			"format": "Untitled"
		},
		{
			"type": "extension",
			"token": "{ext}",
			"description": "Insert file vsnotes.",
			"format": "md"
		}
	],
	"vim.neovimPath": "/usr/local/bin/nvim",
	"redhat.telemetry.enabled": false,
	"[json]": {
		"editor.defaultFormatter": "vscode.json-language-features"
	},
	"editor.guides.indentation": false,
	"eslint.execArgv": null,
	"editor.inlineSuggest.enabled": true,
	"workbench.sideBar.location": "right",
	"cSpell.ignoreWords": ["scanability"],
	"workbench.editor.untitled.hint": "hidden",
	"vscode-pets.petSize": "small",
	"vscode-pets.petType": "clippy",
	"[jsonc]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"indenticator.color.dark": "#2ac3de",
	"vscode-neovim.neovimClean": true,
	"vscode-neovim.neovimExecutablePaths.darwin": "/usr/local/bin/nvim",
	"extensions.experimental.affinity": {
		"asvetliakov.vscode-neovim": 1
	}
}